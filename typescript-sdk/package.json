{
    "name": "@apicurio/apicurio-studio-client",
    "private": false,
    "version": "1-DEV",
    "type": "module",
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "files": [
      "dist"
    ],
    "scripts": {
      "clean": "rimraf dist src-generated .kiota",
      "kiota-info": "cross-env KIOTA_VERSION=v1.11.1 kiota info -l typescript",
      "generate-studio-sdk": "cross-env KIOTA_VERSION=v1.11.1 kiota generate -l typescript -d ../app/src/main/resources-unfiltered/META-INF/apis/studio/v1/studio-api-v1.json -c ApicurioStudioClient -o ./src-generated/studio-client",
      "dev": "vite",
      "build": "tsc --p ./tsconfig-build.json && vite build",
      "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    },
    "peerDependencies": {
      "@microsoft/kiota-abstractions": "1.0.0-preview.66",
      "@microsoft/kiota-http-fetchlibrary": "1.0.0-preview.65",
      "@microsoft/kiota-serialization-form": "1.0.0-preview.54",
      "@microsoft/kiota-serialization-json": "1.0.0-preview.66",
      "@microsoft/kiota-authentication-azure": "1.0.0-preview.61",
      "@microsoft/kiota-serialization-text": "1.0.0-preview.63",
      "@microsoft/kiota-serialization-multipart": "1.0.0-preview.44"
    },
    "devDependencies": {
      "@apicurio/eslint-config": "0.3.0",
      "@kiota-community/kiota-gen": "1.0.1",
      "@microsoft/kiota-abstractions": "1.0.0-preview.66",
      "@microsoft/kiota-http-fetchlibrary": "1.0.0-preview.65",
      "@microsoft/kiota-serialization-form": "1.0.0-preview.54",
      "@microsoft/kiota-serialization-json": "1.0.0-preview.66",
      "@microsoft/kiota-authentication-azure": "1.0.0-preview.61",
      "@microsoft/kiota-serialization-text": "1.0.0-preview.63",
      "@microsoft/kiota-serialization-multipart": "1.0.0-preview.44",
      "@types/node": "22.5.4",
      "@types/react": "18.3.5",
      "@types/react-dom": "18.3.0",
      "@typescript-eslint/eslint-plugin": "8.5.0",
      "@typescript-eslint/parser": "8.5.0",
      "@vitejs/plugin-react": "4.3.1",
      "cross-env": "7.0.3",
      "eslint": "9.10.0",
      "eslint-plugin-react-hooks": "4.6.2",
      "eslint-plugin-react-refresh": "0.4.11",
      "rimraf": "6.0.1",
      "typescript": "5.6.2",
      "vite": "5.4.4",
      "vite-plugin-dts": "4.2.1",
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "react-router-dom": "6.26.2"
    }
  }
